@using sportsShopWebApp.Models


<section>

    <table class="table" style="margin:6vh">
        <thead class="thead-dark">
            <tr class="bg-success"> <th scope="col">TypeOfclothe</th> <th scope="col">Gender</th> <th scope="col">Company</th> <th scope="col">Model</th> <th scope="col">Price</th> <th scope="col">Quantity</th> <th scope="col">IsShort</th> <th scope="col">IsDryfit</th> <th scope="col">ImageData</th> </tr>
        </thead>

        <tbody>
            @foreach (Clothe item in Model)
            {
                <tr scope="row"><td>@item.typeOfclothe</td><td>@item.gender</td><td>@item.company</td><td>@item.model</td><td>@item.price</td><td>@item.quantity</td><td>@item.isShort</td><td>@item.isDryfit</td><td><img src="@item.imageData" style="width:45px;height:45px" /></td></tr>
            }
        </tbody>
    </table>
</section>
